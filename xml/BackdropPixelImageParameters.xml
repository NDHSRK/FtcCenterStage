        <image_source>rear_webcam</image_source>
        <resolution>
            <width>640</width>
            <height>480</height>
        </resolution>
        <image_roi>
            <x>0</x>
            <y>70</y>
            <width>640</width>
            <height>275</height>
        </image_roi>
    </image_parameters>
</backdrop_pixel_image_parameters>